mutation AddArtistAndAlbum {
    insert_artist_one(object: {
      name: "Drake", 
      albums: {data: [{title: "If You're Reading This It's Too Late"}]}
    }) {
      artist_id
      name
      albums {
        album_id
        title
      }
    }
  }

  
